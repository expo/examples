{
  "name": "with-router-tv",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "EXPO_TV=1 expo start",
    "android": "EXPO_TV=1 expo run:android",
    "ios": "EXPO_TV=1 expo run:ios",
    "web": "expo start --web",
    "reset-project": "./scripts/reset-project.js",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "prebuild": "EXPO_TV=1 expo prebuild --clean",
    "deploy": "npx expo export -p web && npx eas-cli@latest deploy"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "expo": "^53.0.4",
    "expo-build-properties": "~0.14.5",
    "expo-constants": "~17.1.3",
    "expo-font": "~13.3.0",
    "expo-linking": "~7.1.3",
    "expo-router": "~5.0.3",
    "expo-splash-screen": "~0.30.6",
    "expo-status-bar": "~2.2.2",
    "expo-system-ui": "~5.0.5",
    "expo-web-browser": "~14.1.5",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "npm:react-native-tvos@0.79.2-0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.3.0",
    "react-native-screens": "~4.10.0",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-tvos/config-tv": "^0.1.1",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.3.1",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.1",
    "react-test-renderer": "^18.3.1",
    "typescript": "~5.8.3"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native"
      ]
    }
  },
  "private": true
}
